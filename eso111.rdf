        <ExtendedRelationOntology:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A Material Entity that is either a natural or man-made feature of the environment.</ExtendedRelationOntology:definition>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Environmental Feature</rdfs:label>
    </owl:Class>
    


