        <ExtendedRelationOntology:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A Geographic Feature associated with water.</ExtendedRelationOntology:definition>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Hydrographic Feature</rdfs:label>
    </owl:Class>
    


