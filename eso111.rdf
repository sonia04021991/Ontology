        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Process Aggregate</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologylibrary.mil/CommonCore/Mid/GeospatialOntology#AtmosphericFeature -->

