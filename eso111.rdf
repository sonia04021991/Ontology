        <rdfs:subClassOf rdf:resource="http://www.ontologylibrary.mil/CommonCore/Mid/ArtifactOntology#LongGun"/>
        <ExtendedRelationOntology:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A Long Gun having a rifled barrel that fires single bullets over long ranges with high accuracy.</ExtendedRelationOntology:definition>
        <ExtendedRelationOntology:definition_source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://en.wikipedia.org/wiki/Rifle</ExtendedRelationOntology:definition_source>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Rifle</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologylibrary.mil/CommonCore/Mid/ArtifactOntology#RocketLauncher -->

    <owl:Class rdf:about="http://www.ontologylibrary.mil/CommonCore/Mid/ArtifactOntology#RocketLauncher">
        <rdfs:subClassOf rdf:resource="http://www.ontologylibrary.mil/CommonCore/Mid/ArtifactOntology#ProjectileLauncher"/>
        <ExtendedRelationOntology:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A Weaponized Launcher that is designed to launch an Unguided Rocket.</ExtendedRelationOntology:definition>
        <ExtendedRelationOntology:definition_source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://en.wikipedia.org/wiki/Rocket_launcher</ExtendedRelationOntology:definition_source>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Rocket Launcher</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologylibrary.mil/CommonCore/Mid/ArtifactOntology#RoundShot -->

    <owl:Class rdf:about="http://www.ontologylibrary.mil/CommonCore/Mid/ArtifactOntology#RoundShot">
        <rdfs:subClassOf rdf:resource="http://www.ontologylibrary.mil/CommonCore/Mid/ArtifactOntology#PortionOfAmmunition"/>
        <ExtendedRelationOntology:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A Portion of Ammunition that is designed to be a solid projectile without explosive charge and to be fired from a cannon.</ExtendedRelationOntology:definition>
        <ExtendedRelationOntology:definition_source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://en.wikipedia.org/wiki/Round_shot</ExtendedRelationOntology:definition_source>
        <rdfs:label>Round Shot</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologylibrary.mil/CommonCore/Mid/ArtifactOntology#SemiAutomaticPistol -->

    <owl:Class rdf:about="http://www.ontologylibrary.mil/CommonCore/Mid/ArtifactOntology#SemiAutomaticPistol">
        <rdfs:subClassOf rdf:resource="http://www.ontologylibrary.mil/CommonCore/Mid/ArtifactOntology#HandGun"/>
        <ExtendedRelationOntology:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A Hand Gun having a single fixed firing chamber machined into the rear of the barrel, and a magazine, usually removable, so they can be used to fire more than one round of ammunition.</ExtendedRelationOntology:definition>
        <ExtendedRelationOntology:definition_source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://en.wikipedia.org/wiki/Semi-automatic_pistol</ExtendedRelationOntology:definition_source>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Semi-automatic Pistol</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologylibrary.mil/CommonCore/Mid/ArtifactOntology#Shell -->

    <owl:Class rdf:about="http://www.ontologylibrary.mil/CommonCore/Mid/ArtifactOntology#Shell">
        <rdfs:subClassOf rdf:resource="http://www.ontologylibrary.mil/CommonCore/Mid/ArtifactOntology#PortionOfAmmunition"/>
        <ExtendedRelationOntology:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A Portion of Ammunition that is designed to be non-self-propelled projectiles and to carry a payload (explosive or other) over relatively short distances.</ExtendedRelationOntology:definition>
        <ExtendedRelationOntology:definition_source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://en.wikipedia.org/wiki/Shell_(projectile)</ExtendedRelationOntology:definition_source>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Shell</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologylibrary.mil/CommonCore/Mid/ArtifactOntology#Shotgun -->

    <owl:Class rdf:about="http://www.ontologylibrary.mil/CommonCore/Mid/ArtifactOntology#Shotgun">
        <rdfs:subClassOf rdf:resource="http://www.ontologylibrary.mil/CommonCore/Mid/ArtifactOntology#LongGun"/>
        <ExtendedRelationOntology:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A Long Gun that fires packets of shot, a single slug, a sabot, or a specialty round (such as tear gas, bolo shell, or a breaching round) over shorter ranges than that of Rifles and with less accuracy.</ExtendedRelationOntology:definition>
        <ExtendedRelationOntology:definition_source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://en.wikipedia.org/wiki/Shotgun</ExtendedRelationOntology:definition_source>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Shotgun</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologylibrary.mil/CommonCore/Mid/ArtifactOntology#Spacecraft -->

    <owl:Class rdf:about="http://www.ontologylibrary.mil/CommonCore/Mid/ArtifactOntology#Spacecraft">
        <rdfs:subClassOf rdf:resource="http://www.ontologylibrary.mil/CommonCore/Mid/ArtifactOntology#Vehicle"/>
        <ExtendedRelationOntology:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A Vehicle that is designed to convey passengers from one location to another by space travel.</ExtendedRelationOntology:definition>
        <ExtendedRelationOntology:definition_source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://en.wikipedia.org/wiki/Spacecraft</ExtendedRelationOntology:definition_source>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Spacecraft</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologylibrary.mil/CommonCore/Mid/ArtifactOntology#SubmachineGun -->

    <owl:Class rdf:about="http://www.ontologylibrary.mil/CommonCore/Mid/ArtifactOntology#SubmachineGun">
        <rdfs:subClassOf rdf:resource="http://www.ontologylibrary.mil/CommonCore/Mid/ArtifactOntology#LongGun"/>
        <ExtendedRelationOntology:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An Automatic Weapon that is a magazine-fed firearm, usually smaller than other automatic firearms, that fires pistol-caliber ammunition.</ExtendedRelationOntology:definition>
        <ExtendedRelationOntology:definition_source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://en.wikipedia.org/wiki/Submachine_gun</ExtendedRelationOntology:definition_source>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Submachine Gun</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologylibrary.mil/CommonCore/Mid/ArtifactOntology#Tank -->

    <owl:Class rdf:about="http://www.ontologylibrary.mil/CommonCore/Mid/ArtifactOntology#Tank">
        <rdfs:subClassOf rdf:resource="http://www.ontologylibrary.mil/CommonCore/Mid/ArtifactOntology#GroundMotorVehicle"/>
        <ExtendedRelationOntology:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A Ground Vehicle that is designed to be used for front-line combat which combines operational mobility, tactical offensive, and defensive capabilities. Firepower is normally provided by a large-calibre main gun in a rotating turret and secondary machine guns, while heavy armour and all-terrain mobility provide protection for the tank and its crew, allowing it to perform all primary tasks of the armoured troops on the battlefield (derived from http://en.wikipedia.org/wiki/Tank).</ExtendedRelationOntology:definition>
        <ExtendedRelationOntology:definition_source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://en.wikipedia.org/wiki/Tank</ExtendedRelationOntology:definition_source>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Tank</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologylibrary.mil/CommonCore/Mid/ArtifactOntology#TorpedoTube -->

    <owl:Class rdf:about="http://www.ontologylibrary.mil/CommonCore/Mid/ArtifactOntology#TorpedoTube">
        <rdfs:subClassOf rdf:resource="http://www.ontologylibrary.mil/CommonCore/Mid/ArtifactOntology#ProjectileLauncher"/>
        <ExtendedRelationOntology:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A Weaponized Launcher that is designed to launch torpedoes.</ExtendedRelationOntology:definition>
        <ExtendedRelationOntology:definition_source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://en.wikipedia.org/wiki/Torpedo_tube</ExtendedRelationOntology:definition_source>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Torpedo Tube</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologylibrary.mil/CommonCore/Mid/ArtifactOntology#Train -->

    <owl:Class rdf:about="http://www.ontologylibrary.mil/CommonCore/Mid/ArtifactOntology#Train">
        <rdfs:subClassOf rdf:resource="http://www.ontologylibrary.mil/CommonCore/Mid/ArtifactOntology#RailTransportVehicle"/>
        <ExtendedRelationOntology:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A Rail Transport Vehicle that is designed to convey cargo or passengers from one location to another.</ExtendedRelationOntology:definition>
        <ExtendedRelationOntology:definition_source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://en.wikipedia.org/wiki/Train</ExtendedRelationOntology:definition_source>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Train</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologylibrary.mil/CommonCore/Mid/ArtifactOntology#TransportationFacility -->

    <owl:Class rdf:about="http://www.ontologylibrary.mil/CommonCore/Mid/ArtifactOntology#TransportationFacility">
        <rdfs:subClassOf rdf:resource="http://www.ontologylibrary.mil/CommonCore/Mid/ArtifactOntology#Facility"/>
        <ExtendedRelationOntology:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A Facility that is designed for commencing or concluding the transportation of transportation artifacts, or for housing transportation artifacts.</ExtendedRelationOntology:definition>
        <ExtendedRelationOntology:definition_source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://en.wikipedia.org/wiki/Transport</ExtendedRelationOntology:definition_source>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Transportation Facility</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologylibrary.mil/CommonCore/Mid/ArtifactOntology#Truck -->

    <owl:Class rdf:about="http://www.ontologylibrary.mil/CommonCore/Mid/ArtifactOntology#Truck">
        <rdfs:subClassOf rdf:resource="http://www.ontologylibrary.mil/CommonCore/Mid/ArtifactOntology#GroundMotorVehicle"/>
        <ExtendedRelationOntology:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A Ground Vehicle that is designed to be used to transport cargo and vary greatly in size, power and configuration, and may be configured to mount specialized equipment, such as in the case of fire trucks and concrete mixers and suction excavators. (derived from http://en.wikipedia.org/wiki/Truck)</ExtendedRelationOntology:definition>
        <ExtendedRelationOntology:definition_source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://en.wikipedia.org/wiki/Truck</ExtendedRelationOntology:definition_source>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Truck</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologylibrary.mil/CommonCore/Mid/ArtifactOntology#UnguidedRocket -->

    <owl:Class rdf:about="http://www.ontologylibrary.mil/CommonCore/Mid/ArtifactOntology#UnguidedRocket">
        <rdfs:subClassOf rdf:resource="http://www.ontologylibrary.mil/CommonCore/Mid/ArtifactOntology#PortionOfAmmunition"/>
        <ExtendedRelationOntology:alternative_label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Rocket</ExtendedRelationOntology:alternative_label>
        <ExtendedRelationOntology:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A Portion of Ammunition that is designed to be a self-propelled, but unguided, projectile that delivers some payload (explosive or other) over relatively long distances.</ExtendedRelationOntology:definition>
        <ExtendedRelationOntology:definition_source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://en.wikipedia.org/wiki/Rocket</ExtendedRelationOntology:definition_source>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Unguided Rocket</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologylibrary.mil/CommonCore/Mid/ArtifactOntology#Vehicle -->

    <owl:Class rdf:about="http://www.ontologylibrary.mil/CommonCore/Mid/ArtifactOntology#Vehicle">
        <rdfs:subClassOf rdf:resource="http://www.ontologylibrary.mil/CommonCore/Mid/ArtifactOntology#Artifact"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologylibrary.mil/CommonCore/Upper/ExtendedRelationOntology#bearer_of"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologylibrary.mil/CommonCore/Mid/ArtifactOntology#ConveyanceArtifactFunction"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <ExtendedRelationOntology:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A Transportation Artifact that is designed to facilitate the movement of material entities from one location to another by conveying them there.</ExtendedRelationOntology:definition>
        <ExtendedRelationOntology:definition_source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://en.wikipedia.org/wiki/Vehicle</ExtendedRelationOntology:definition_source>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vehicle</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologylibrary.mil/CommonCore/Mid/ArtifactOntology#Watercraft -->

    <owl:Class rdf:about="http://www.ontologylibrary.mil/CommonCore/Mid/ArtifactOntology#Watercraft">
        <rdfs:subClassOf rdf:resource="http://www.ontologylibrary.mil/CommonCore/Mid/ArtifactOntology#Vehicle"/>
        <ExtendedRelationOntology:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A Vehicle that is designed to convey passengers from one location to another by water travel.</ExtendedRelationOntology:definition>
        <ExtendedRelationOntology:definition_source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://en.wikipedia.org/wiki/Watercraft</ExtendedRelationOntology:definition_source>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Watercraft</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologylibrary.mil/CommonCore/Mid/ArtifactOntology#Weapon -->

